memory int n end
memory int result end
memory int up_to end

proc fac int -> int do
  # Calculates n!
  1 while over 1 > do
    over *
    swap 1 -
    swap
  end
  swap drop
end

proc main do
  1 n !int
  1 result !int
  20 up_to !int

  while n ?int up_to ?int < do
    result ?int n ?int * result !int
    result ?int puti "\n" puts drop
    n ?int 1 + n !int
  end

  10 fac puti "\n" puts drop
end
