memory n 8 end
memory result 8 end
memory up_to 8 end

1 n !64
1 result !64
20 up_to !64

while n ?64 up_to ?64 < do
  result ?64 n ?64 * result !64
  result ?64 puti "\n" puts
  n ?64 1 + n !64
end

proc fac int -> int do
  # Calculates n!
  1 while over 1 > do
    over *
    swap 1 -
    swap
  end
  swap drop
end

10 fac puti "\n" puts
